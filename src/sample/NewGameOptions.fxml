<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="back2mainMenuBtn" layoutX="14.0" layoutY="410.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="26.0" prefWidth="110.0" text="Main Menu" />
      <Button fx:id="playGameBtn" layoutX="310.0" layoutY="410.0" mnemonicParsing="false" onAction="#playGame" prefHeight="26.0" prefWidth="110.0" text="Start Game" AnchorPane.rightAnchor="15.0" />
      <Label layoutX="136.0" layoutY="28.0" text="New Game Options" textFill="WHITE">
         <font>
            <Font name="DejaVu Sans Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="p1name" layoutX="126.0" layoutY="89.0" prefHeight="26.0" prefWidth="218.0" promptText="Enter Player1 name" text="Player1" />
      <TextField fx:id="p2name" layoutX="126.0" layoutY="134.0" prefHeight="26.0" prefWidth="217.0" promptText="Enter Player2 name" text="Player2" />
      <Circle fill="#ff1f1f" layoutX="69.0" layoutY="102.0" radius="13.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
      <Circle fill="DODGERBLUE" layoutX="69.0" layoutY="147.0" radius="13.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
